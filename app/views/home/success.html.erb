    <body class="no-sidebar">

		<!-- Header -->
			<div id="header">

				<!-- Inner -->
					<div class="inner">
						<header>
							<h1><a href="#" id="logo">Mkv</a></h1>
						</header>
					</div>
				
				<!-- Nav -->
					<nav id="nav">
						<ul>
							<li><a href="/">Home</a></li>
                            <% if session['fb_access_token'] %>
                                <li><a href="/signout">Sign Out</a></li>
                            <% end %>
						</ul>
					</nav>

			</div>
			
		<!-- Main -->
			<div class="wrapper style1">

				<div class="container">
					<div class="row">
						<div class="12u skel-cell-mainContent" id="content">
							<hr />
							<article id="main" class="special">
								<header>
									<h2>Success Page</h2>
									<span class="byline">
                                    <% if session['fb_access_token'] %>
    									<ul>
                                            <% flash.each do |key, msg| %>
                                              <%= content_tag :li, msg, id: key %>
                                            <% end %>
                                        </ul><br/>
                                        <a class="button" href="/share">Share</a>
                                    <% else %>
                                        You have not logged in using facebook go to home page and login
                                    <% end %>
									</span>
								</header>
							</article>
						</div>
					</div>
				</div>

			</div>

		<!-- Footer -->
			<div id="footer">
				<div class="container">
					<div class="row">
						<div class="12u">
							
							<!-- Contact -->
								<section class="contact">
									<ul class="icons">
										<li><a href="#" class="icon icon-github-sign"><span>Github</span></a></li>
										<li><a href="#" class="icon icon-facebook"><span>Facebook</span></a></li>
										<li><a href="#" class="icon icon-google-plus"><span>Google+</span></a></li>
										<li><a href="#" class="icon icon-linkedin"><span>Linkedin</span></a></li>
									</ul>
								</section>
							
							<!-- Copyright -->
								<div class="copyright">
									<ul class="menu">
										<li>&copy; mkv. All rights reserved.</li>
									</ul>
								</div>
							
						</div>
					
					</div>
				</div>
			</div>

	</body>